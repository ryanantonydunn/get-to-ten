<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ryanantonydunn.gettoten" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Get To Ten</name>
    <description>
        Tap adjacent tiles with the same number to get a bigger number, try to get to ten.
    </description>
    <author email="ryanantonydunn@gmail.com" href="https://ryanantonydunn.github.io/get-to-ten">
        Ryan Dunn
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <preference name="android-targetSdkVersion" value="28" />
        <preference name="android-minSdkVersion" value="19" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="ios" />
    <plugin name="cordova-plugin-admobpro" spec="^2.37.2">
        <variable name="PLAY_SERVICES_VERSION" value="16.0.0" />
    </plugin>
    <engine name="android" spec="^7.0.0" />
</widget>
